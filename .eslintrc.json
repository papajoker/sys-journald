{
  "ecmaFeatures": {
   "jsx": true,
    "modules": true
  },
  "env": {
    "es6": true,
    "node": true,
    "jquery": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "camelcase": 2,
    "comma-spacing": [
      2,
      {
        "after": true,
        "before": false
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "complexity": 2,
    "computed-property-spacing": [
      2,
      "never"
    ],
    "eol-last": 2,
    "func-names": 0,
    "func-style": [
      2,
      "declaration"
    ],
    "indent": [1,4],
    "key-spacing": 2,
    "keyword-spacing": [
      2,
      {
        "after": true,
        "before": true
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "lines-around-comment": 2,
    "max-len": 0,
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "quote-props": 0,
    "quotes": 0,
    "semi": [1, "never"],
    "semi-spacing": [
      2,
      {
        "after": true,
        "before": false
      }
    ],
    "space-before-blocks": 2,
    "space-before-function-paren": 2,
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": [
      2,
      "always"
    ],
    "strict": 2,
    "yield-star-spacing": 2
  }
}
